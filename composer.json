  "support": {
    "email": "chezrd@gmail.com",
    "issues": "https://github.com/ChezRD/jivochat-webhooks-api/issues",
    "source": "https://github.com/ChezRD/jivochat-webhooks-api",
    "docs": "https://github.com/ChezRD/jivochat-webhooks-api"
  },
  "require": {
    "php": "^7.3 || ^8",
    "doctrine/collections": "^1.8"
  },
  "require-dev": {
    "ext-pdo": "*",
    "monolog/monolog": "1.9.*",
    "ext-mongodb": "*",
    "mongodb/mongodb": "1.1.*",
    "phpunit/phpunit": "~6.1",
    "fzaninotto/faker": "^1.6"
  },
  "suggest": {
    "ext-pdo": "Allow logging of Webhooks requests/response data to a MySQL server",
    "monolog/monolog": "Allow logging of Webhooks requests/response using Monolog",
    "ext-mongodb": "Allow logging of Webhooks requests/response data to a MongoDB server",
    "mongodb/mongodb": "Allow logging of Webhooks requests/response data to a MongoDB server via PHP Driver"
  },
  "autoload": {
    "psr-4": {
      "ChezRD\\Jivochat\\Webhooks\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
